import java.util.ArrayList;


public class Compteur {
	public String nom;
	public ArrayList<Type> listetype;
	
	public Compteur(String n, ArrayList<Type> l){
		nom=n;
		listetype=l;
	}
	
	public ArrayList<Type> getType(){
		return listetype;
	}
	
	public String getNomCompt(){
		return nom;
	}
	
	public void addType(Type t){
		listetype.push(t);
	}
}

package WP;


public class Writer extends Thread{
	private Buffer buf;
	private int identité;
	
	public Writer(Buffer c, int n) {
		buf = c;
		this.identité = n;
	}
	
	public void run() {
	try {
		buf.write("message");
	} catch (InterruptedException e) {
		e.printStackTrace();
	}
	System.out.println("Producteur #" + this.identité + " met : " + "message");
	}
}


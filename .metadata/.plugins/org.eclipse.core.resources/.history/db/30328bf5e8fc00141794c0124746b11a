

public class Writer extends Thread{
	public Writer(String name){
		super(name);
	}
	
	public void run(){
		ReaderWriter test = new ReaderWriter();
		int i = 0;
		while(true){
			try {
				i++;
				test.write("element "+i);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}

		}
	}

}

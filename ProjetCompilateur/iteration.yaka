PROGRAMME iter1
VAR ENTIER s, i, n, j;
	ECRIRE("n=");
	LIRE(n);

	i = 1; s =0;
	TANTQUE i<= n FAIRE
		s = s + i;

		TANTQUE j <= n FAIRE
			j = j + 3;
		FAIT;

		i = i + 1;
	FAIT;
	ALALIGNE;
	ECRIRE("s=");
	ECRIRE(s);
	i = 1; s =0;
	TANTQUE i<= n FAIRE
		s = s + i;

		TANTQUE j <= n FAIRE
			j = j + 3;
		FAIT;

		i = i + 1;
	FAIT;
FPROGRAMME
